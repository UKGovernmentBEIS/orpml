<orpml 
xmlns="http://www.beis.gov.uk/namespaces/orpml" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instanc"
DocumentURI="http://www.beis.gov.uk/ukpga/Eliz2/6-7/51"
IdURI="http://www.legislation.gov.uk/id/ukpga/Eliz2/6-7/51"
xsi:schemaLocation="http://www.beis.gov.uk/namespaces/orpml"
SchemaVersion=“1.0”
xmlns:dc=“http://purl.org/dc/elements/1.1/“>

  <xs:element name="DublinCore">
    <xs:complexType>
      <xs:all>
        <dc:identifier minOccurs="1" maxOccurs="1"/>
        <dc:title minOccurs="1" maxOccurs="1"/>
        <dc:description minOccurs="0" maxOccurs="1"/>
        <dc:date minOccurs="1" maxOccurs="1"/>
        <dc:type minOccurs="1" maxOccurs="1"/>
        <dc:format minOccurs="1" maxOccurs="1"/>
        <dc:language minOccurs="1" maxOccurs="1"/>
        <dc:publisher minOccurs="1" maxOccurs="1"/>
        <dc:modified minOccurs="1" maxOccurs="1"/>
        <dc:contributor minOccurs="1" maxOccurs="1"/>
        <dc:valid minOccurs="0" maxOccurs="1"/>
      </xs:all>
    </xs:complexType>
  </xs:element>

  <!-- define HTML content type-->
  <xs:complexType name="HTMLType" mixed="true">
    <xs:all>
      <xs:any namespace="http://www.w3.org/1999/xhtml" minOccurs="0" maxOccurs="unbounded"/>
    </xs:all>
  </xs:complexType>

  <!-- define ContentSection type-->
  <xs:complexType name="ContentSectionType" type="HTMLType" minOccurs="0" maxOccurs="unbounded"/>

  <!-- define release-note content type-->
  <xs:complexType name="ReleaseNoteType" mixed="true">
    <xs:sequence>
      <xs:element name="version" type="HTMLType" minOccurs="1" maxOccurs="1"/>
      <xs:element name="date" type="HTMLType" minOccurs="1" maxOccurs="1"/>
      <xs:element name="details" type="HTMLType" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <!-- document level metadata (apart from DC above) -->
  <xs:element name="Metadata">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Formally">
          <xs:complexType>
            <xs:all>
              <xs:element name="PreviousIdentifier" type="HTMLType" minOccurs="0" maxOccurs="unbounded"/>
            </xs:all>
          </xs:complexType>
        </xs:element>
        <xs:element name="Revision" type="HTMLType" minOccurs="0" maxOccurs="1"/>
        <xs:element name="Feedback" type="HTMLType" minOccurs="0" maxOccurs="1"/>
        <xs:element name="Copyright" type="HTMLType" minOccurs="0" maxOccurs="1"/>
        <xs:element name="Contents">
          <xs:complexType>
            <xs:choice>
              <xs:element name="ContentSection" type="ContentSectionType" minOccurs="0" maxOccurs="unbounded"/>
              <xs:element name="ContentSubSection">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ContentSection" type="ContentSectionType" minOccurs="0" maxOccurs="unbounded"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:choice>
          </xs:complexType>
        </xs:element>
        <xs:element name="ReleaseNotes">
          <xs:complexType>
            <xs:all>
              <xs:element name="ReleaseNote" type="ReleaseNoteType" minOccurs="0" maxOccurs="unbounded"/>
            </xs:all>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!-- Document markup goes here -->
  <xs:element name="DocumentContent" type="HTMLType" minOccurs="1" maxOccurs="1"/>
</orpml>
